{
    "name": "timebook-api",
    "version": "1.0.0",
    "description": "This is simple REST API example for AWS Lambda By Serverless framwork with TypeScript and MongoDB Atlas.",
    "main": "serverless.ts",
    "scripts": {
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "local": "serverless offline --host 0.0.0.0 --httpPort 3001 --skipCacheInvalidation",
        "deploy": "serverless deploy",
        "postinstall": "npm run schema:generate",
        "schema:migrate": "npx prisma migrate dev --skip-seed",
        "schema:reset": "npx prisma migrate reset --skip-seed",
        "schema:seed": "npx prisma db seed --preview-feature",
        "schema:generate": "npx prisma generate",
        "schema:format": "npx prisma format",
        "test": "echo \"Error: no test specified\" && exit 1",
        "test-backup-script": "cross-env NODE_ENV=dev mocha -r ts-node/register tests/*.test.ts --exit",
        "coverage": "nyc --reporter lcov npm run test"
    },
    "pre-commit": [
        "lint"
    ],
    "dependencies": {
        "@graphql-tools/merge": "^6.2.14",
        "@graphql-tools/schema": "^7.1.5",
        "@graphql-tools/utils": "^7.10.0",
        "@middy/core": "^2.0.0-alpha.3",
        "@middy/do-not-wait-for-empty-event-loop": "^2.0.0-alpha.3",
        "@middy/http-json-body-parser": "^2.0.0-alpha.3",
        "@prisma/client": "^2.27.0",
        "algoliasearch": "^4.10.3",
        "apollo-datasource": "^3.0.0",
        "apollo-datasource-rest": "^3.0.0",
        "apollo-server-cache-redis": "^3.0.0",
        "apollo-server-caching": "^3.0.0",
        "apollo-server-core": "^3.0.0",
        "apollo-server-lambda": "^3.0.0",
        "apollo-server-plugin-response-cache": "^3.0.0",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "dotenv": "^10.0.0",
        "graphql": "^15.5.1",
        "handlebars": "^4.7.7",
        "jsonwebtoken": "^8.5.1",
        "stripe": "^8.157.0",
        "zb-email-verifier": "^0.6.2"
    },
    "devDependencies": {
        "@serverless/typescript": "2.29.0",
        "@types/aws-lambda": "^8.10.51",
        "@types/chai": "^4.2.11",
        "@types/jsonwebtoken": "^8.5.4",
        "@types/lambda-tester": "^3.6.0",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.14.6",
        "@types/sinon": "^9.0.0",
        "@types/supertest": "^2.0.8",
        "aws-sdk": "^2.839.0",
        "chai": "^4.2.0",
        "cross-env": "^7.0.2",
        "fork-ts-checker-webpack-plugin": "^6.0.0",
        "istanbul": "^0.4.5",
        "json-schema-to-ts": "^1.4.1",
        "lambda-tester": "^4.0.1",
        "mocha": "^7.1.2",
        "nyc": "^15.0.1",
        "prisma": "^2.27.0",
        "serverless": "^2.29.0",
        "serverless-domain-manager": "^5.1.0",
        "serverless-offline": "^6.8.0",
        "serverless-webpack": "^5.2.0",
        "sinon": "^9.0.2",
        "ts-loader": "^8.0.10",
        "ts-node": "^8.9.1",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "tslint": "^6.1.2",
        "typescript": "^4.3.4",
        "webpack": "^5.4.0",
        "webpack-node-externals": "^2.5.2"
    }
}
