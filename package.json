{
    "name": "timebook-api",
    "version": "1.0.0",
    "description": "PocketSeq API",
    "main": "serverless.ts",
    "scripts": {
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "local": "serverless offline --host 0.0.0.0 --httpPort 3001",
        "deploy": "serverless deploy",
        "postinstall": "npm run schema:generate",
        "schema:migrate": "npx prisma migrate dev --skip-seed",
        "schema:reset": "npx prisma migrate reset --skip-seed",
        "schema:seed": "npx prisma db seed",
        "schema:generate": "npx prisma generate",
        "schema:format": "npx prisma format",
        "sls:deploy": "npx serverless deploy --aws-profile JRG_AWS",
        "test": "mocha",
        "coverage": "nyc npm run test"
    },
    "pre-commit": [
        "lint"
    ],
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "dependencies": {
        "@graphql-tools/merge": "^6.2.17",
        "@graphql-tools/schema": "^7.1.5",
        "@graphql-tools/utils": "^7.10.0",
        "@middy/core": "^3.0.0",
        "@middy/do-not-wait-for-empty-event-loop": "^3.0.0",
        "@middy/http-json-body-parser": "^3.0.0",
        "@prisma/client": "^3.15.0",
        "algoliasearch": "^4.13.0",
        "apollo-datasource": "^3.3.0",
        "apollo-datasource-rest": "^3.5.0",
        "apollo-server-cache-redis": "^3.3.0",
        "apollo-server-caching": "^3.3.0",
        "apollo-server-core": "^3.7.0",
        "apollo-server-lambda": "^3.7.0",
        "apollo-server-plugin-response-cache": "^3.5.0",
        "axios": "^0.26.0",
        "bcryptjs": "^2.4.3",
        "dotenv": "^10.0.0",
        "graphql": "^15.8.0",
        "graphql-map-selections": "^1.1.8",
        "handlebars": "^4.7.7",
        "ioredis": "^4.28.0",
        "jsonwebtoken": "^8.5.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.3",
        "sharp": "^0.30.0",
        "stripe": "^8.219.0",
        "zb-email-verifier": "^0.7.0"
    },
    "devDependencies": {
        "@serverless/typescript": "^2.71.0",
        "@types/aws-lambda": "^8.10.100",
        "@types/chai": "^4.3.0",
        "@types/ioredis": "^4.28.0",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lambda-tester": "^3.6.0",
        "@types/lodash": "^4.14.180",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.18.0",
        "@types/sharp": "^0.30.0",
        "@types/sinon": "^9.0.0",
        "@types/supertest": "^2.0.8",
        "aws-sdk": "^2.1120.0",
        "chai": "^4.3.6",
        "cross-env": "^7.0.2",
        "fork-ts-checker-webpack-plugin": "^6.5.0",
        "json-schema-to-ts": "^1.4.1",
        "lambda-tester": "^4.0.1",
        "mocha": "^7.2.0",
        "nyc": "^15.1.0",
        "prisma": "^3.15.0",
        "serverless": "^2.71.0",
        "serverless-domain-manager": "^5.8.0",
        "serverless-offline": "^8.7.0",
        "serverless-webpack": "^5.7.0",
        "sinon": "^9.0.0",
        "source-map-support": "^0.5.21",
        "ts-loader": "^8.3.0",
        "ts-node": "^8.10.0",
        "tsconfig-paths-webpack-plugin": "^3.5.0",
        "tslint": "^6.1.2",
        "typescript": "^4.6.2",
        "webpack": "^5.72.0",
        "webpack-node-externals": "^2.5.2"
    }
}
