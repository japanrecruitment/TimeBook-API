{
    "name": "timebook-api",
    "version": "1.0.0",
    "description": "PocketSeq API",
    "main": "serverless.ts",
    "scripts": {
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "local": "serverless offline --host 0.0.0.0 --httpPort 3001",
        "deploy": "serverless deploy",
        "schema:migrate": "npx prisma migrate dev --skip-seed",
        "schema:reset": "npx prisma migrate reset --skip-seed",
        "schema:seed": "npx prisma db seed",
        "schema:generate": "npx prisma generate",
        "schema:format": "npx prisma format",
        "test": "mocha",
        "coverage": "nyc npm run test"
    },
    "pre-commit": [
        "lint"
    ],
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "dependencies": {
        "@graphql-tools/merge": "^6.2.14",
